package Zadatci;

public class Zadatak2 {
	
	public static void main(String[] args) {
		
		// Program unosi dva cijela broja.
		// Program ispisuje OK ako je zbroj svih parnih brojeva
		// između ta dva cijela broja
		// veći od 1000
		
		int i=2;
		int j=1000000;
		
		int zbroj=0;
		
		if(i<j) {
			for(int a=i; a<=j; a++) {
				if(a%2==0) {
					zbroj+=zbroj;
				}
			
			}
		
		if(zbroj<10) {
			System.out.println("ok");
		}
			
		}
		
		
	}

}
