package edunova;

public class E02ForPetlja {

	public static void main(String[] args) {
	
	for(int i=0; i<10; i++)	{
		System.out.println("Osijek");
	}
	
	
	for(int i=0; i<10; i++)	{
		System.out.println(i);
	}
	
	
	int zbroj = 0;
	
	for(int i=1; i<=100;i++) {
		zbroj +=i;
	}
	System.out.println(zbroj);
	
	
		

	}

}
