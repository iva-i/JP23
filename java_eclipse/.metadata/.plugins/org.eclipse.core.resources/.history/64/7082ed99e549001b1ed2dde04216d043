package zimski_zadatci;

import java.util.Arrays;

public class Zadatak05 {

	public static void main(String[] args) {
		// Kreirati program koji ispisuje cikličnu matricu - ulaz
		// su dva cijela broja
		
		int redak=4, stupac=3;
		
		int[][] m = new int[redak][stupac];
		
		int brojac=redak*stupac;
		int ponavljanje=1;
		
		int redakM=redak-1, stupacM=stupac-1, redakm=0, stupacm=0;
		
		
		
		while(brojac>0) {
			
			//(redak) desno -> lijevo
			for(int i=redakM; i>=redakM; i--) {
				for(int j=stupacM; j<stupacm; j--) {
					m[i][j]=brojac;
					brojac=brojac-1;
						if(m[i][j]>0) {
							return;
						}
				}
			}
			
			
			
		}
	}
	
}
			
			
			
			
			
			
			
			
			
			
			
			
			
			
				
			}
			
		}
		
		
		
		
		
		
		
		
		

	}
}
		
	
		
		
		



